{
  "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
  "diagnostics": {
    "disable": [
      "unnecessary-if",
      "unnecessary-assert"
    ]
  },
  "runtime": {
    "requirePattern": [
      "lua/?.lua",
      "lua/?/init.lua"
    ],
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "${3rd}/luv/library",
      "${3rd}/busted/library",
      "${3rd}/luassert/library"
    ]
  }
}